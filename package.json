{
  "name": "@playlyfe/gql",
  "version": "2.0.0",
  "description": "gql service and tools",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mayank1791989/gql.git"
  },
  "scripts": {
    "test": "yarn run check-code && npm run testonly -- --coverage",
    "testonly": "cross-env NODE_ENV=test jest",
    "check-code": "yarn run lint",
    "flow-check": "flow check",
    "lint": "eslint src",
    "flow-typed-update": "flow-typed update",
    "build": "yarn run clean && babel ./src --out-dir ./lib --ignore '**/__**__/**,**/node_modules/**'",
    "report-coverage": "codecov",
    "semantic-release": "semantic-release pre && npm publish --access public && semantic-release post",
    "clean": "rm -rf lib",
    "commit": "git-cz",
    "precommit": "yarn run test"
  },
  "author": "Mayank Agarwal <mayank1791989@gmail.com>",
  "license": "MIT",
  "files": [
    "lib",
    "README.md"
  ],
  "dependencies": {
    "apollo-codegen": "0.10.13",
    "babel-runtime": "6.23.0",
    "codemirror-graphql": "0.6.4",
    "dentist": "1.0.3",
    "fb-watchman": "2.0.0",
    "find-config": "1.0.0",
    "flow-runtime": "0.12.0",
    "graphql": "0.9.6",
    "invariant": "2.2.2",
    "json5": "0.5.1",
    "keymirror": "0.1.1",
    "leven": "2.1.0",
    "lodash": "4.17.4",
    "minimatch": "3.0.3",
    "node-ipc": "9.0.1",
    "parse-glob": "3.0.4",
    "promise-retry": "1.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.0",
    "babel-plugin-flow-runtime": "0.11.1",
    "babel-polyfill": "6.23.0",
    "babel-preset-playlyfe": "1.2.0",
    "codecov": "2.1.0",
    "commitizen": "2.9.6",
    "cross-env": "4.0.0",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.1.3",
    "flow-bin": "0.45.0",
    "flow-typed": "2.1.2",
    "husky": "0.13.3",
    "jest": "20.0.0",
    "semantic-release": "6.3.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testRegex": "__tests__/.*.test.js$",
    "testEnvironment": "node",
    "roots": [
      "src/"
    ]
  }
}
