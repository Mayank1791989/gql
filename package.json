{
  "name": "@playlyfe/gql",
  "version": "3.0.0",
  "description": "gql service and tools",
  "main": "lib/index.js",
  "bin": {
    "gql": "./lib/gql-cli/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mayank1791989/gql.git"
  },
  "scripts": {
    "test": "yarn run check-code && yarn run testonly -- --coverage",
    "testonly": "yarn run clean-test-temp-dir && cross-env NODE_ENV=test jest",
    "check-code": "yarn run lint",
    "flow-check": "flow check",
    "lint": "jest --config jest-eslint.config.js",
    "flow-typed-update": "rm -rf flow-typed/npm && flow-typed update && babel-node ./scripts/fix-flow-typed.js",
    "build": "yarn run clean && babel ./src --out-dir ./lib --ignore '**/__**__/**,**/node_modules/**'",
    "report-coverage": "codecov",
    "semantic-release": "semantic-release",
    "clean": "rm -rf lib",
    "clean-test-temp-dir": "babel-node ./scripts/clear-test-temp-dir",
    "commit": "git-cz",
    "precommit": "yarn run test",
    "travis-deploy-once": "travis-deploy-once"
  },
  "author": "Mayank Agarwal <mayank1791989@gmail.com>",
  "license": "MIT",
  "files": [
    "lib",
    "README.md"
  ],
  "dependencies": {
    "apollo-codegen": "0.19.1",
    "babel-runtime": "6.26.0",
    "chokidar": "2.0.4",
    "code-frame": "5.0.0",
    "dentist": "1.0.3",
    "fb-watchman": "2.0.0",
    "fbemitter": "2.1.1",
    "find-config": "1.0.0",
    "flow-runtime": "0.17.0",
    "glob-parent": "3.1.0",
    "globby": "8.0.1",
    "graphql": "0.13.2",
    "graphql-language-service-interface": "1.1.0",
    "graphql-language-service-parser": "1.1.0",
    "import-from": "2.1.0",
    "invariant": "2.2.4",
    "json5": "1.0.1",
    "leven": "2.1.0",
    "lodash": "4.17.10",
    "log-symbols": "2.2.0",
    "micromatch": "3.1.10",
    "minimatch": "3.0.4",
    "normalize-path": "3.0.0",
    "parse-glob": "3.0.4",
    "plur": "3.0.1",
    "promise-retry": "1.1.1",
    "sane": "2.5.2",
    "semver": "5.5.0",
    "uuid": "3.3.2",
    "yargs": "12.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-jest": "23.4.0",
    "babel-plugin-flow-runtime": "0.17.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-playlyfe": "2.0.4",
    "codecov": "3.0.4",
    "commitizen": "2.10.1",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.19.1",
    "eslint-plugin-playlyfe": "4.1.1",
    "flow-bin": "0.76.0",
    "flow-typed": "2.5.1",
    "fs-extra": "7.0.0",
    "has-ansi": "3.0.0",
    "husky": "0.14.3",
    "jest": "23.4.0",
    "jest-runner-eslint": "0.6.0",
    "jest-watch-typeahead": "0.2.0",
    "memory-fs": "0.4.1",
    "mkdirp": "0.5.1",
    "object-hash": "1.3.0",
    "prettier": "1.13.7",
    "semantic-release": "15.7.1",
    "strip-ansi": "4.0.0",
    "travis-deploy-once": "5.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testRegex": "__tests__/.*.test.js$",
    "testEnvironment": "node",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "snapshotSerializers": [
      "<rootDir>/scripts/jest-gql-snapshot-serializer"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/src"
    ],
    "cacheDirectory": "<rootDir>/.tmp/jest-cache",
    "roots": [
      "<rootDir>/src/"
    ]
  }
}
